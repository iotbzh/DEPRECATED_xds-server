{
    "version": "0.2.0",
    "configurations": [{
            "name": "XDS-Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}",
            "env": {
                "GOPATH": "${workspaceRoot}/../../../..:${env:GOPATH}",
                "ROOT_DIR": "${workspaceRoot}/../../../.."
            },
            "args": ["-log", "debug"],
            "showLog": false
        },
        {
            "name": "XDS-Server local dev",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}",
            "env": {
                "GOPATH": "${workspaceRoot}/../../../..:${env:GOPATH}",
                "ROOT_DIR": "${workspaceRoot}/../../../..",
                "XDS_LOG_SILLY": "0"
            },
            "args": ["-log", "debug", "-c", "__config_local_dev.json"],
            "showLog": false
        }
    ]
}
